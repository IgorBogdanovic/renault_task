<section class="m-obj-media  utl-float-left  utl-clearfix">

  <div class="_m-obj-media__img  o-media__wrapper--box-shadow-3  utl-float-left">
    {{> atoms-img-490x410 }}
  </div>

  <div class="_m-obj-media__txt  glb-max-width-320  glb-margin-top-160  glb-margin-left-22  utl-float-left">
    <h6 class="a-cmp-heading--5">{{ media-section-heading.tenth }}</h6>
    <p class="a-cmp-paragraph  a-cmp-paragraph--default  m-obj-media__paragraph">{{ media-section-paragraph.second }}</p>
  </div>

  <div class="m-obj-media__counter  glb-margin-top-160  utl-float-left">
    <span> {{> atoms-caret-left }} <span class="utl-color-dark-yellow">01&nbsp;&nbsp;</span>/&nbsp;&nbsp;05 {{> atoms-caret-right }} </span>
  </div>

</section>